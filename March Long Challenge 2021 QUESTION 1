QUESTION 1 - 
MY SOLUTION - 
#include<bits/stdc++.h>
using namespace std;
int solve()
{
  int N,H,X;
  cin >> N >> H >> X;
  int T[N];
  for(int i=0;i<N;i++)
  {
    cin >> T[i];
  }
  for(int i=0;i<N;i++)
  {
    if((T[i]+X)>=H)
    {
      return 1;
    }
  }
  return 0;
}
int main()
{
  int a;
  a = solve();
  if(a==0)
  {
    cout<< "NO";
  }
  else if(a==1)
  {
    cout<< "YES";
  }
}


CODECHEF SOLUTION - 
#include<bits/stdc++.h>
using namespace std;
void solve()
{
  int N,H,X;
  cin >> N >> H >> X;
  int T[N];
  for(int i=0;i<N;i++)
  {
    cin >> T[i];
  }
  int flag = 0;
  for(int i=0;i<N;i++)
  {
    if((T[i]+X)>=H)
    {
      flag = 1;
    }
  }
  if(flag)
  {
    cout << "YES";
  }
  else 
  {
    cout << "NO";
  }
}
int main()
{
  solve();
  cout << "\n";
}
