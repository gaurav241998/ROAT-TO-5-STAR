PROBLEM 1  - 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int T;
    cin>>T;
    
    while(T--)
    {
        int w=0;
    int x=0;
    int y=0;
    int z=0;
    string s;
    cin>>s;
    int count=0;
    int m=s.size();
    
    if(m<10)
    {
        cout<<"NO"<<endl;
        continue;
    }
    for(int i=0;i<m;i++)
    {
        if(s[i]=='@' || s[i]=='#' || s[i]=='%' || s[i]=='&' || s[i]=='?')
        {
            if(i>0 && i<m-1 && w==0)
            {
                w=1;
                count++;
            }
        }
        else if((int)s[i]>=97 && (int)s[i]<=122)
        {
            if(x==0)
            {
               x=1;
               count++;
            }
        }
        else if(s[i]=='1' || s[i]=='2' || s[i]=='3' || s[i]=='4' || s[i]=='5' || s[i]=='6' || s[i]=='7' || s[i]=='8' || s[i]=='9' || s[i]=='0')
        {
            if(i>0 && i<m-1 && y==0)
            {
                y=1;
                count++;
            }
        }
        else if((int)s[i]>=65 && (int)s[i]<=90)
        {
            if(i>0 && i<m-1 && z==0)
            {
                z=1;
                count++;
            }
        }
        
    }
    //cout<<count;
    
    if(count==4)
    {
        cout<<"YES"<<endl;
    }
    else if(count<4)
    {
        cout<<"NO"<<endl;
    }
    }
    return 0;
}

PROBLEM 2 - 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int T;
    cin>>T;
    
    while(T--)
    {
      int L,R;
      cin >> L >> R;
      int sum = 0;
      int a = L+L;
      int b = R+R;
      sum += b-a+1;
      cout << sum;
      cout << "\n";
    }
    return 0;
}

